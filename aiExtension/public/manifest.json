{
    "manifest_version": 3,
    "name": "Bihari Ai Extension",
    "version": "1.0",
    "description": "Shows how to display the side panel only on google.com using the Side Panel API.",
    "background": {
        "service_worker": "service-worker.js"
    },
    "action": {
        "default_title": "Click to Open Side Panel",
        "default_popup": "index.html"
    },
    "permissions": [
        "sidePanel",
        "tabs",
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+B",
                "mac": "Command+B"
            }
        }
    },
    "web_accessible_resources": [
        {
            "resources": [
                "index.html"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "icons": {
        "16": "Tejasvi Gradient DP.png",
        "48": "Tejasvi Gradient DP.png",
        "128": "Tejasvi Gradient DP.png"
    }
}